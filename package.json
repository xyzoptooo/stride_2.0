{
  "name": "semester-stride-backend",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
  "start": "cross-env NODE_ENV=production node app.js",
    "dev": "cross-env NODE_ENV=development nodemon index.js",
  "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles",
    "lint": "eslint .",
    "format": "prettier --write .",
    "build": "cross-env NODE_ENV=production npm install --production",
    "build:dev": "cross-env NODE_ENV=development npm install",
    "security-check": "npm audit",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.56.1",
    "axios": "^1.12.2",
    "base-64": "^1.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "file-type": "^21.0.0",
    "helmet": "^7.1.0",
    "mammoth": "^1.11.0",
    "mongoose": "^8.18.0",
    "multer": "^2.0.2",
    "node-cron": "^3.0.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.0",
    "pdf-parse": "^1.1.1",
    "react-datepicker": "^8.7.0",
    "tesseract.js": "^6.0.1",
  "ioredis": "^5.3.2",
    "winston": "^3.17.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "cross-env": "^10.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.2.0",
    "supertest": "^6.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
